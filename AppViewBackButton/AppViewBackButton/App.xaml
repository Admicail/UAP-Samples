<Application
    x:Class="AppViewBackButton.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AppViewBackButton"
    RequestedTheme="Light"
    xmlns:converters="using:AppViewBackButton.Converters"
	xmlns:locator="using:AppViewBackButton.ViewModels.Base">
    <Application.Resources>
        <ResourceDictionary>

            <!-- ViewModelLocator -->
            <locator:ViewModelLocator x:Key="Locator" />

            <!-- Converters -->
            <converters:IntToVisibilityConverter x:Key="IntToVisibilityConverter" />

        </ResourceDictionary>
    </Application.Resources>
</Application>